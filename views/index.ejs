<%- include("header"); -%>
	<div class="box" id="heading">
	<h1><%= listTitle %> </h1>
	</div>
	
	<div class="box">

	<% for (var i=0; i < ourNewItems.length; i++) { %>  
	<div class="item">
	  <input type="checkbox">
			<p> <%= ourNewItems[i] %> </p>
	</div>
		<% } %>
	<form class="item" action="/"  method="post">
